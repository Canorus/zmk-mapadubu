#include "mapadubu.dtsi"
#include "mapadubu_layout.dtsi"

/ {
    chosen {
        zmk,layout = &keymap_layout;
    };
};

&kscan0 {
	matrix_transform = &matrix_transform;
};

&matrix_transform {
	columns = <5>;
	rows = <4>;
	map = <
        RC(0,5) RC(0,6) RC(0,7) RC(0,8) RC(0,9)
        RC(1,5) RC(1,6) RC(1,7) RC(1,8) RC(1,9)
        RC(2,5) RC(2,6) RC(2,7) RC(2,8) RC(2,9)
        RC(3,5) ZMK_MATRIX_NODE_NOOP RC(3,7) RC(3,8) RC(3,9)
	>;
};
